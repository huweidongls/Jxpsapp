<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="240.0dip"
    android:layout_height="fill_parent"
    android:background="@drawable/bitmap_book_read_chapterlist_repeat"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="240.0dip"
        android:layout_height="45.0dip"
        android:background="@drawable/newbgtop"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="45.0dip"
            android:layout_marginLeft="30.0dip"
            android:gravity="center_vertical"
            android:text="用户名:"
            android:textColor="#ffffffff"
            android:textSize="14.0sp" />

        <TextView
            android:id="@id/username"
            android:layout_width="wrap_content"
            android:layout_height="45.0dip"
            android:layout_marginLeft="10.0dip"
            android:gravity="center_vertical"
            android:text="guanghe"
            android:textColor="#ffffffff"
            android:textSize="14.0sp" />
    </LinearLayout>

    <ScrollView
        android:id="@id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="40.0dip"
                android:gravity="center"
                android:visibility="gone">

                <TextView
                    android:id="@id/menu_search"
                    android:layout_width="190.0dip"
                    android:layout_height="32.0dip"
                    android:background="@drawable/slide_search_bg"
                    android:clickable="true"
                    android:drawablePadding="1.0dip"
                    android:gravity="center_vertical"
                    android:paddingLeft="15.0dip"
                    android:paddingRight="10.0dip"
                    android:text="  搜索店铺"
                    android:textColor="#ffffffff"
                    android:textSize="16.0sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="40.0dip"
                android:background="@drawable/state_select"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@id/open"
                    android:layout_width="0.0dip"
                    android:layout_height="20.0dip"
                    android:layout_weight="1.0"
                    android:src="@drawable/kaigong01" />

                <ImageView
                    android:id="@id/close"
                    android:layout_width="0.0dip"
                    android:layout_height="20.0dip"
                    android:layout_weight="1.0"
                    android:src="@drawable/shougong" />
            </LinearLayout>

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider" />

            <TextView
                android:id="@id/menu_shoplist"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawableLeft="@drawable/slide_icon_restraunt"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  订单管理"
                android:textColor="#ffffffff"
                android:textSize="16.0sp" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider" />

            <TextView
                android:id="@id/menu_orderslist"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawableLeft="@drawable/slide_icon_orders"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  待发货订单"
                android:textColor="#ffffffff"
                android:textSize="16.0sp"
                android:visibility="gone" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider"
                android:visibility="gone" />

            <TextView
                android:id="@id/menu_about"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawableLeft="@drawable/slide_icon_about"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  订单统计"
                android:textColor="#ffffffff"
                android:textSize="16.0sp" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider" />

            <TextView
                android:id="@id/menu_userinfo"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawableLeft="@drawable/slide_icon_login"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  个人中心"
                android:textColor="#ffffffff"
                android:textSize="16.0sp" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider" />

            <TextView
                android:id="@id/menu_contact"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawableLeft="@drawable/slide_icon_service"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  客服热线"
                android:textColor="#ffffffff"
                android:textSize="16.0sp"
                android:visibility="gone" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider"
                android:visibility="gone" />

            <TextView
                android:id="@id/menu_out"
                android:layout_width="240.0dip"
                android:layout_height="50.0dip"
                android:background="@drawable/selector_category_item"
                android:clickable="true"
                android:drawablePadding="1.0dip"
                android:gravity="center_vertical"
                android:paddingLeft="15.0dip"
                android:paddingRight="10.0dip"
                android:text="  退出登录"
                android:textColor="#ffffffff"
                android:textSize="16.0sp" />

            <View
                android:layout_width="240.0dip"
                android:layout_height="1.0dip"
                android:background="@drawable/ic_shelf_category_divider" />
        </LinearLayout>
    </ScrollView>

    <SeekBar
        android:id="@id/seekBar1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />
</LinearLayout>